          <tbody>
            <h2>Withdrawl</h2>
            <tr>
              <th>Amount</th>
              <th>Date</th>
            </tr>
              <%@user.withdrawls.each do |withdrawl|%>
                <tr>
                  <th> <%=withdrawl.amount%></th>
